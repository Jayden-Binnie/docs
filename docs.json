{
  "$schema": "https://mintlify.com/.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "defaultColorScheme": "dark",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Most Important",
            "pages": [
              "index",
              "reception-sop"
            ]
          },
          {
            "group": "Operations",
            "pages": [
              "Operations/pausing-payments",
              "Operations/failed-payments"
            ]
          },
          {
            "group": "Sales",
            "pages": [
              "Sales/our-offer"
            ]
          }
        ]
      },
      {
        "tab": "What's New",
        "groups": [
          {
            "group": "Updates",
            "pages": [
              "whats-new",
              "release-notes"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Martial Logic",
          "href": "https://martiallogic.com",
          "icon": "globe"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/MartialLogicLogo.svg",
    "dark": "/logo/MartialLogicLogo.svg"
  },
  "homepage": "/",
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}